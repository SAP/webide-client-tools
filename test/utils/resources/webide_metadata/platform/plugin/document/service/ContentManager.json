{
	"name" : "sap.watt.common.plugin.document.service.ContentManager",
	"description" : "The content manager service interface",

	"configurationProperties" : {
		"maxAllowedSizeOfCachedContent" : {
			"type" : "number",
			"multiple" : false
		},
		"maxAllowedSizeOfCachedFileContent" : {
			"type" : "number",
			"multiple" : false
		},
		"cleanOnTotalCacheExceededLimit" : {
			"type" : "number",
			"multiple" : false
		},
		"aFilesToBeRemovedFirst" : {
			"type" : "[string]",
			"multiple" : false
		},
		"aFilesContentAlwaysInCache" : {
			"type" : "[string]",
			"multiple" : false
		}
	},

	"methods" : {
		"deleteContentData" : {
			"description": "deletes document cached content",			
			"params" : [{
				"name" : "oDocument",
				"type" : "sap.watt.common.plugin.document.Document"
			}]
		}
	}
}